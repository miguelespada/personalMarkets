<%= page_title(t("Photo"), Photo.icon) %>

<div class="main-container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <div class="panel panel-default panel-table-last center">
        <div class="panel-body">
            <%= photo(@photo, 400) %>
            <%= render "photos/utils/actions", photo: @photo, photographic: true  %>
        </div>
        <div>
          <%= before_form_link %>
          <%= all_photos_link %>
          <%= user_photos_link(current_user) %>
        </div>
      </div>
    </div>
  </div>
</div>