<section>
  <h2><%= role.titleize %></h2>
  <table class="table table-striped table-bordered table-hover <%= role %>">
    <tr>
    <th>Email</th>
    <th>Status</th>
    <th>Market Count</th>
    <th>Number of likes</th>
    <th>Actions</th>
    </tr>

    <% users.each do |user| %>
        <%= render user %>
    <% end %>
  </table>
</section>

