<section>
  <div class="panel panel-default panel-table">
    <div class="panel-heading table-title">
      <%= role.titleize %>
    </div>
    <div class="table-container">
      <table class="table table-striped">
        <tr>
        <th>Email</th>
        <th>Status</th>
        <th>Market Count</th>
        <th>Number of likes</th>
        <th>Actions</th>
        </tr>
      
        <% users.each do |user| %>
          <%= render user %>
        <% end %>
      </table>
    </div>
  </div>
</section>



