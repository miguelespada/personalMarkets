<div class = "coupon_item col-md-4">
  <div class = "show gallery_coupon_photo">
  <%= link_to coupon.market do %>
    <%= photo(coupon.photography, 300, 150) %>

    <div class = "home_gallery_label">
      <span><%= coupon.market.name %></span>
    </div>
    <% end %>
  </div> 

  <div class="coupon-gallery-extra">
    <%= render partial: "coupons/shared/description", locals: {coupon: coupon } %>
    <%= coupon.market.decorate.buy_coupon_link %>
  </div>
</div>