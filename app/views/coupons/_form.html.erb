<%= f.input :description, { placeholder: "Description" } %>
<%= t :Price %>: <%= f.input :price, collection: (5..25).step(5), :include_blank => false %>
<%= t :Available %>: <%= f.input :available, collection: (10..100).step(5), :include_blank => false %>

<%= f.simple_fields_for :photography do |form_builder| %>
  <%= render 'photos/form', f: form_builder %>
<% end %>