<div class="panel panel-default panel-table panel-table-last">

 <div class="panel-heading table-title">
    Make people's wishes come true, recommend markets. 
  </div>


  <div class = "home_gallery wish_gallery">
    <% @wishes.each do |wish| %>
        <div class = "home_gallery_item wish_item">
          <div class = "home_gallery_featured_photo wish_featured_photo" data-name="<%= wish.description %>">
            <%= photo(wish.photography, 300, 150) %>
            <div class = "home_gallery_label wish_label">
              <%= link_to wish.description, wish_path(wish) %>
            </div>
          </div>
        </div>
    <% end %>
  </div>

</div>


