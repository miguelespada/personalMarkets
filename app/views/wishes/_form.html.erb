<%= simple_form_for [current_user, @wish] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :description %>
    <%= f.input :wish_photo, as: :attachinary %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>


<script>
$( document ).ready(function() {
  $('.attachinary-input').attachinary();
});
</script>