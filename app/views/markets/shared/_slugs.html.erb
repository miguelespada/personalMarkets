<table class="table table-striped table-bordered table-hover">
  <tr>
  <th>Actions</th>
  <th>State</th>
  <th>Featured photo</th>
  <th>Gallery</th>
  <th>Name</th>
  <th>Description</th>
  <th>Owner</th>
  <th>Category</th>
  <th>Tags</th>
  <th>Owner</th>
  <th>Address</th>
  <th>City</th>
  <th>Latitude</th>
  <th>Longitude</th>
  <th>Date</th>
  <th>Coupon</th>
  <th>Coupon available</th>
  <th>Coupon price</th>
  <th>Coupon photo</th>
  <th>Like counter</th>
  </tr>
  <% markets.each do |market| %>
    <% market = market.decorate %>
    <%= content_tag_for(:tr, market) do %>
      <td> <%= market.actions %> </td>
      <td> <%= market.state %> </td>
      <td> <%= photo(market.featured, 100, 50) %></td>
      <td> <%= market.link(market.name)%> </td>
      <td> <%= market.description %></td>
      <td> <%= market.owner %></td>
      <td> <%= market.category_name %> </td>
      <td> <%= market.tags %> </td>
      <td> <%= market.owner %> </td>
      <td> <%= market.address %> </td>
      <td> <%= market.city %> </td>
      <td> <%= market.latitude %> </td>
      <td> <%= market.longitude %> </td>
      <td> <%= market.date %> </td>
      <td> <%= link_to  market.coupon.description, coupon_path(market.coupon) if market.coupon? %> </td>
      <td> <%= market.coupon.available if market.coupon? %> </td>
      <td> <%= market.coupon.price if market.coupon? %> </td>
      <td> <%= photo(market.coupon.photography, 100, 50)  if market.coupon? %> </td>
      <td class="like-counter"> <%= market.favorited.count %> </td>
    <% end %>
  <% end %>
</table>