<div class="panel-heading table-title">
  <div class="title-icon">
    <%= image_tag "favicon.png" %>
  </div>
  
  <div class="row">
    <div class="market-title col-md-6">
      <%= market.link(market.formatted_market_name)%>  
    </div>
    
    <div class="market-actions col-md-6">
      <%= market.social_links %>
      <%= market.like_link %>
    </div>
  </div>
  
  <div class="row">
    <div class="market-actions col-md-12">
      <%= market.actions %>
    </div>
  </div>

</div>