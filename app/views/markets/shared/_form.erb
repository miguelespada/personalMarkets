<%= simple_form_for [user, market], 
  :html => {"data-tags" => "#{raw_tags(market)}", 
          "data-suggested-tags" => "#{raw_suggested_tags(@suggested_tags)}" }  do |f| %>
  <%= f.input :name, input_html: { class: 'market-name' }%>
  <%= f.input :description, label_html: { class: 'market-description' } %>
  <%= f.association :category, :include_blank => false %>
  <%= f.input :featured, as: :attachinary %>
  <%= f.input :longitude %>
  <%= f.input :latitude %>  
  <%= f.input :tags, input_html: { class: 'tm-input' } %>
  <%= f.button :submit %>
<% end %>