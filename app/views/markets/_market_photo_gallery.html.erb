<div class="market-section-title">Gallery</div>
<div class="market-section-container">
	<div id="carousel-container-market-photo-gallery" class="carousel slide col-md-6" data-ride="carousel">
		<div class="carousel-inner">
			<% market.gallery.photographies.each do |photo| %>
				<div class="item">
	  			<%= photo(photo, 800, 400) %>
	  		</div>
			<% end %>
		</div>
  </div>

  <div class="col-md-6 market-photos">
  	<% market.gallery.photographies.each do |photo| %>
	  	<%= photo(photo, 120, 60) %>
		<% end %>
  </div>
</div>

<%= javascript_include_tag "explore_slideshows" %>

<style>
@media (max-width:992px){
	.market-photos{
	  padding: 0px !important;
	}
}
</style>