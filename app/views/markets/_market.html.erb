<div> <%= market.name %> </div>
<div class="featured-photo"> <%= market.featured_photo(image_size) %></div>

<div> 
  <p class = "label"><%= Market.human_attribute_name(:description) %> </p>  
  <%= market.description %> 
</div>

<div class = "market-longitude"> <%= market.longitude %> </div>
<div class = "market-latitude"> <%= market.latitude %> </div>
<div class = "market-user-email"> <%= market.category.name %> </div>  
<div> <%= market.tags %> </div> 
<div class = "market-user-email"> <%= market.user.email %> </div>
<div> <%= market.show_link %> </div>
<div> <%= market.edit_link %> </div>
<div> <%= market.delete_link %> </div>
<div> <%= market.like_link %>  </div>
<div> <%= market.qr_code_link %>  </div>

<div> 
  <span> Number of likes: </span>  
  <span> <%= market.favorited.count %>  </span>
</div>

<div class="calendar"> 
  <span> <%= market.name %>  </span>
  <span> <%= market.date %>  </span>
</div>
