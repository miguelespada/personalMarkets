<%= stylesheet_link_tag 'maps'%>
<%= stylesheet_link_tag 'markets/tooltip'%>
<%= javascript_include_tag 'maps' %>

<div id="map"></div>

<div id="map_theme_buttons" class="btn-group-vertical">
  <% Category.each do |category| %>
   <button type="button" class="theme btn btn-success btn-sm" id="<%= category.style %>" title="<%= category.name %>">
      <span class="glyphicon <%= category.glyph %>"><p class="hidden"><%= category.name %></p></span>
   </button>
  <% end %>
</div>