<h4>Tags</h4>
<% @tags.each do |tag| %>
  <%= content_tag(:span, class: "tag", id: tag) do %>  
       <%= link_to tag, markets_search_path({"query" => tag }) %>
  <% end %>
<% end %>

<h4>Suggested Tags</h4>

<div id = "suggested-tags">
    <% @starred.each do |tag| %>
      <%= content_tag(:span, class: "tag", id: tag) do %>  
           <%= link_to tag, markets_search_path({"query" => tag }) %>
      <% end %>
    <% end %>
</div>